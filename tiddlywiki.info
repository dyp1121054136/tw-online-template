{  
    "description": "我的 TiddlyWiki 站点",  
    "plugins": [  
        "tiddlywiki/highlight"  
    ],  
    "themes": [  
        "tiddlywiki/vanilla",  
        "tiddlywiki/snowwhite"  
    ],  
    "languages": [  
        "zh-Hans"  
    ],  
    "includeWikis": [],  
    "build": {    
        "index": [    
              "--setfield", "[tag[image]]", "_canonical_uri", "$:/core/templates/canonical-uri-external-image", "text/plain",  
              "--setfield", "[tag[image]]", "text", "", "text/plain",    
              "--rendertiddler", "$:/core/save/all", "index.html", "text/plain"    
        ]    
     }
}
